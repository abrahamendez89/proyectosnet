                                <Path
                                        Fill="{StaticResource ToolBar_InnerGripOrnament}"
                                        Margin="1 1 0 0"
                                        Width="14"
                                        Height="2"
                                        VerticalAlignment="Top"
                                        HorizontalAlignment="Left"
                                        Data="M12,0 L12,2 14,2 14,0 12,0 z M8,0 L8,2 10,2 10,0 8,0 z M4,0 L4,2 6,2 6,0 4,0 z M0,0 L0,2 2,2 2,0 0,0 z"/>
