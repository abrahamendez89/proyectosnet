                <Path
                        Fill="#FFFFFFFF"
                        Stretch="Fill"
                        Margin="2,5,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Width="5"
                        Height="3"
                        Data="M0,0 L5,0 5,1 4,1 4,2 3,2 3,3 2,3 2,2 1,2 1,1 0,1 0,0 z"/>
                <Path
                        Fill="{StaticResource ControlElement_Normal}"
                        Stretch="Fill"
                        Margin="0,4,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Width="5"
                        Height="3"
                        Data="M0,0 L5,0 5,1 4,1 4,2 3,2 3,3 2,3 2,2 1,2 1,1 0,1 0,0 z"/>
                <Path Fill="#FFFFFFFF" Stretch="Fill" Margin="2,1,0,0" HorizontalAlignment="Center" VerticalAlignment="Top" Width="5" Height="1" Data="M0,0 L5,0 5,1 0,1 0,0 z"/>
                <Path
                        Fill="{StaticResource ControlElement_Normal}"
                        Stretch="Fill"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Width="5"
                        Height="1"
                        Data="M0,0 L5,0 5,1 0,1 0,0 z"/>
            </Grid>
        </Grid>
    </ControlTemplate>
